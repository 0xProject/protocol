{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["__build__/**", "dist/**", "out/**", "public/build/**"]
        },
        "build:no-diff": {
            "dependsOn": ["^build:no-diff"]
        },
        "fix": {
            "cache": false
        },
        "test:ci": {
            "dependsOn": ["build"]
        },
        "lint:ci": {}
    }
}
