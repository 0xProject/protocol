{
    "name": "@0x/protocol-types",
    "version": "1.0.0",
    "engines": {
        "node": ">=6.12"
    },
    "description": "0x protocol types",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "tsc -b",
        "build:ci": "yarn build",
        "clean": "shx rm -rf lib",
        "lint": "tslint --format stylish --project .",
        "diff_docs": "git diff --exit-code ./docs",
        "s3:sync_md_docs": "aws s3 sync ./docs s3://docs-markdown/${npm_package_name}/v${npm_package_version} --profile 0xproject --region us-east-1 --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers",
        "docs:md": "ts-doc-gen --sourceDir='$PROJECT_FILES' --output=$MD_FILE_DIR --fileExtension=mdx --tsconfig=./typedoc-tsconfig.json",
        "docs:json": "typedoc --excludePrivate --excludeExternals --excludeProtected --ignoreCompilerErrors --target ES5 --tsconfig typedoc-tsconfig.json --json $JSON_FILE_PATH $PROJECT_FILES"
    },
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/0xProject/tools.git"
    },
    "bugs": {
        "url": "https://github.com/0xProject/tools/issues"
    },
    "homepage": "https://github.com/0xProject/tools/tree/main/packages/protocol-types",
    "devDependencies": {
        "@0x/ts-doc-gen": "^0.0.28",
        "@0x/tslint-config": "^4.1.3",
        "shx": "^0.2.2",
        "tslint": "5.11.0",
        "typedoc": "~0.16.11",
        "typescript": "3.0.1"
    },
    "dependencies": {
        "@0x/types": "^3.3.0",
        "@types/node": "12.12.54",
        "bignumber.js": "~9.0.0",
        "ethereum-types": "^3.3.3"
    },
    "publishConfig": {
        "access": "public"
    },
    "gitHead": "4f91bfd907996b2f4dd383778b50c479c2602b56"
}
