        "test:ci": ". ./turbo-prerun.sh && turbo run test:ci"
    },
    "devDependencies": {
        "turbo": "1.8.8",
        "prettier": "^2.8.4",
        "prettier-plugin-tailwindcss": "^0.2.4"
