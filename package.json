{
    "name": "0x-labs",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps-node/*",
        "packages/*",
        "sites/*"
    ],
    "scripts": {
        "build": ". ./turbo-prerun.sh && turbo run build",
        "build:no-diff": ". ./turbo-prerun.sh && turbo run build:no-diff",
        "fix": ". ./turbo-prerun.sh && turbo run fix",
